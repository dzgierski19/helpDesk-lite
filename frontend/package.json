{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.js",
    "start:test": "API_PROXY_TARGET=http://127.0.0.1:9000 concurrently -k \"php -S 127.0.0.1:9000 -t ../backend/public\" \"ng serve --proxy-config proxy.conf.js\"",
    "build": "ng build",
    "set-api-url": "node ./scripts/set-api-url.js",
    "railway-build": "npm run set-api-url && npm run build -- --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e": "playwright test",
    "storybook": "ng run frontend:storybook",
    "build-storybook": "ng run frontend:build-storybook",
    "deploy-storybook": "npm run build-storybook && gh-pages -d storybook-static -f"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "material-icons": "^1.13.12",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.17",
    "@angular/cli": "^17.3.17",
    "@angular/compiler-cli": "^17.3.0",
    "@chromatic-com/storybook": "^1.9.0",
    "@playwright/test": "^1.56.1",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/addon-interactions": "^8.3.5",
    "@storybook/addon-links": "^8.3.5",
    "@storybook/addon-onboarding": "^8.3.5",
    "@storybook/angular": "^8.3.5",
    "@storybook/blocks": "^8.3.5",
    "@storybook/test": "^8.3.5",
    "@types/jasmine": "~5.1.0",
    "concurrently": "^9.2.1",
    "gh-pages": "^6.1.1",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "storybook": "^8.3.5",
    "typescript": "~5.4.2"
  }
}
